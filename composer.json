{
	"name": "hurah/php-typed-property-toggle",
	"description": "Comment / uncomment PHP typed properties for your project.",
	"type": "library",
	"require": {
		"symfony/console": "^5.1@dev",
		"psr/log": "^1.1@dev",
		"monolog/monolog": "^2.0@dev",
		"php" : ">=7.3",
		"composer/composer": "^2.0@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.2@dev"
	},
	"authors": [
		{
			"name": "Anton Boutkam",
			"email": "anton@nui-boutkam.nl"
		}
	],
	"minimum-stability": "dev",
	"autoload-dev": {
		"psr-4": {
			"Test\\": "tests"
		}
	},
	"bin": [
		"bin/transpile"
	],
	"autoload": {
		"psr-4": {
			"Hurah\\Transpile\\": "src/Transpile"
		}
	},
	"license" : "MIT"
}
